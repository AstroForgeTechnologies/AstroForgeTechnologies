{
  "name": "astroforgetechnologies",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "tinaDev": "tinacms dev -c \"astro dev --port 8080\"",
    "dev": "astro dev --port 8080",
    "start": "astro dev --port 8080",
    "build": "astro check && astro build && jampack ./dist",
    "tinaBuild": "astro check && tinacms build && astro build && jampack ./dist",
    "preview": "astro preview --port 8080",
    "buildAndPreview": "astro check && astro build && jampack ./dist && astro preview --port 8080",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "husky install",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "^0.5.5",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/rss": "^4.0.2",
    "@astrojs/vercel": "^7.5.0",
    "@resvg/resvg-js": "^2.6.0",
    "astro": "^4.2.1",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.6",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.11",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tinacms": "^1.5.29",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@astrojs/react": "^3.0.9",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@divriots/jampack": "^0.23.2",
    "@react-three/drei": "^9.99.5",
    "@react-three/fiber": "^8.15.19",
    "@tailwindcss/typography": "^0.5.10",
    "@tinacms/cli": "^1.5.39",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.19",
    "@types/slug": "^5.0.7",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "astro-eslint-parser": "^0.16.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "node-emoji": "^2.1.3",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slug": "^8.2.3",
    "three": "^0.162.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,astro}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  }
}
